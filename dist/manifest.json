{
  "manifest_version": 3,
  "name": "Quick Peek Extension",
  "version": "1.0",
  "description": "A browser extension for quick link previews.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"], "host_permissions": [
    {
      "host": "<all_urls>",
      "operations": ["tab"]
    }
  ]
}
